    <artifactId>ngbootstrap</artifactId>
    <name>PMC: AngularJS for Twitter Bootstrap</name>
    <!--<version>2.3.2</version>-->
    <description>AngularJS (Angular) native directives for Twitter Bootstrap</description>

    <build>
        <resources>
            <resource>
                <directory>ng-bootstrap</directory>
                <filtering>true</filtering>
                <includes>
                    <include>ui-bootstrap-tpls-0.6.0.min.js</include>
                    <include>img/glyphicons-halflings.png</include>
                    <include>img/glyphicons-halflings-white.png</include>
                </includes>
            </resource>
        </resources>
    </build>

    
</project>